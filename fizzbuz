def fizz_buzz(n)
  if n % 15 == 0
    'FizzBuzz'
  elsif n % 3 == 0
    'Fizz'
  elsif n % 5 == 0
    'Buzz'
  else
    n.to_s
  end
end

puts fizz_buzz(1)
puts fizz_buzz(2)
puts fizz_buzz(3)
puts fizz_buzz(4)
puts fizz_buzz(5)
puts fizz_buzz(15)

def default_args(a, b, c=0, d=0)
  puts "a=#{a}, b=#{b}, c=#{c}, d=#{d}"
end

default_args(2,4)

require 'date'
puts Date.today


numbers.each do |n|
  puts sum += n
end

